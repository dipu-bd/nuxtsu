{
  "name": "nuxtsu",
  "description": "A template based on Nuxt with full typescript support for both client and server side",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "backpack dev -r dotenv/config",
    "build": "backpack build && nuxt build",
    "start": "node -r dotenv/config build/main.js",
    "prestart": "npm run build",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.4.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "nuxt": "2.6.2",
    "nuxt-property-decorator": "2.1.3",
    "ts-node": "8.1.0",
    "vuex-class": "0.3.2"
  },
  "devDependencies": {
    "@nuxt/typescript": "2.6.2",
    "@nuxtjs/eslint-config": "0.0.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/node": "11.13.7",
    "@types/webpack-env": "1.13.9",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "backpack-core": "0.8.3",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.2",
    "prettier": "1.17.0",
    "pug": "2.0.3",
    "pug-plain-loader": "1.0.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2"
  }
}
